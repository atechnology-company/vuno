{
  "name": "vuno",
  "version": "0.1.0-alpha",
  "description": "the simple, unitary text editor for the modern age, designed by humans for humans.",
  "packageManager": "pnpm@10.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:vite": "vite dev --port 5175",
    "dev": "npm run dev:vite",
    "build": "vite build",
    "build:speed": "cross-env OPTIMIZATION_MODE=speed vite build",
    "build:size": "cross-env OPTIMIZATION_MODE=size vite build",
    "build:fast": "cross-env OPTIMIZATION_MODE=compile-speed vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri build",
    "tauri:dev": "tauri dev",
    "dev:all": "turbo run dev:vite tauri:dev --parallel",
    "tauri:build": "set NODE_ENV=production && npm run build && tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:debug": "npm run build && tauri build --debug",
    "tauri:build:release": "set NODE_ENV=production && npm run build && tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:speed": "set NODE_ENV=production && npm run build:speed && tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:size": "set NODE_ENV=production && npm run build:size && tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:fast": "set NODE_ENV=production && npm run build:fast && tauri build --target x86_64-pc-windows-msvc",
    "clean": "rimraf node_modules .svelte-kit",
    "lint": "svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "editor",
    "IDE",
    "Rust",
    "Svelte",
    "Tauri"
  ],
  "author": "Max Lee Abdurrahman As-Salafi Carter",
  "license": "MIT",
  "dependencies": {
    "@codemirror/autocomplete": "^6.7.1",
    "@codemirror/commands": "^6.2.4",
    "@codemirror/lang-cpp": "^6.0.2",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.5",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-javascript": "^6.1.9",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-liquid": "^6.2.3",
    "@codemirror/lang-markdown": "^6.2.0",
    "@codemirror/lang-php": "^6.0.1",
    "@codemirror/lang-python": "^6.1.3",
    "@codemirror/lang-rust": "^6.0.1",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-wast": "^6.0.2",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.8.0",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/legacy-modes": "^6.5.0",
    "@codemirror/lint": "^6.4.2",
    "@codemirror/search": "^6.5.0",
    "@codemirror/state": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.13.0",
    "@lezer/common": "^1.2.0",
    "@tauri-apps/api": "^1.5.3",
    "comlink": "^4.4.1",
    "highlight.js": "^11.8.0",
    "marked": "^9.0.3",
    "marked-gfm-heading-id": "^3.0.6",
    "marked-highlight": "^2.0.6",
    "shiki": "^0.14.5",
    "y-codemirror.next": "^0.3.2",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.10"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.3",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tauri-apps/cli": "^1.5.9",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.10.8",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.4",
    "terser": "^5.39.0",
    "turbo": "^2.5.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
